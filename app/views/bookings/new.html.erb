<h1>Bookings#new</h1>
<p>Find me in app/views/bookings/new.html.erb</p>

<%= simple_form_for [@car,@booking] do |f| %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :car_id, :as => :hidden, :input_html => { :value => @car.id }  %>
  <%= f.submit "Book this car!", class: "btn btn-primary" %>
<% end %>
